    "scripts": {
        "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
        "build": "tsc",
        "start": "node build/server.js",
        "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
        "typeorm-run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
        "typeorm-create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
        "typeorm-revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/multer": "^1.4.7",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "eslint": "^7.21.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.7",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "csv-parse": "^5.3.8",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.6.2",
        "tsyringe": "^4.7.0",
        "typeorm": "^0.2.35",
        "uuid": "^9.0.0"
    }
