{
    "name": "rentcar",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
        "build": "tsc",
        "start": "node build/shared/infra/http/server.ts",
        "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm-run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
        "typeorm-create": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
        "typeorm-revert": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
        "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
        "seed:admin": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seed/admin.ts"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/multer": "^1.4.7",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "eslint": "^7.21.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.7",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@types/nodemailer": "^6.4.10",
        "bcrypt": "^5.1.0",
        "csv-parse": "^5.3.8",
        "dayjs": "^1.11.9",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "handlebars": "^4.7.8",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.5",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.6.2",
        "tsyringe": "^4.7.0",
        "typeorm": "^0.2.35",
        "uuid": "^9.0.0"
    }
}
